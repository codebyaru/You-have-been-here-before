func _ready():
	# Thoda wait karte hain taaki game fully load ho jaye (0.1 sec)
	await get_tree().create_timer(0.1).timeout
	
	print(\"ðŸ¤– AI: Simulating 'pause' input...\")
	
	# 1. 'Press' event create karo
	var event_press = InputEventAction.new()
	event_press.action = \"ui_cancel\"  # Make sure Input Map mein ye naam same ho
	event_press.pressed = true
	
	# 2. Engine ko feed karo (Press)
	Input.parse_input_event(event_press)
	
	# 3. 'Release' event create karo (Taaki button dab ke na reh jaye)
	var event_release = InputEventAction.new()
	event_release.action = \"ui_cancel\"
	event_release.pressed = false
	
	# 4. Engine ko feed karo (Release)
	Input.parse_input_event(event_release)
"

